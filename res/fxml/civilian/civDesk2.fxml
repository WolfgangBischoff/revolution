<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.canvas.Canvas?>
<StackPane xmlns="http://javafx.com/javafx"
           xmlns:fx="http://javafx.com/fxml">

    <ImageView>
        <Image url="@/img/desk.png" />
    </ImageView>

    <ImageView translateX="-130" translateY="80">
        <Image url="@/img/medicine.png" />
    </ImageView>

    <ImageView translateX="80" translateY="-0">
        <Image url="@/img/pen.png" fx:id="pen"/>
    </ImageView>

    <ImageView translateX="-170" translateY="-85" preserveRatio="true" fitHeight="135">
        <Image url="@/img/diffuser.png" />
    </ImageView>

    <ImageView fx:id="smokeSprite" translateY="-185" translateX="-150" preserveRatio="true" fitHeight="80"/>

    <ImageView translateY="10" translateX="-20" preserveRatio="true">
        <Image  url="@/img/mobilephone.png"/>
    </ImageView>

</StackPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>

<?import java.net.URL?>
<GridPane xmlns:fx="http://javafx.com/fxml" fx:id="gridpane" hgap="5">

    <stylesheets>
        <URL value="@/css/grayBackgroundAndButton.css"/>
    </stylesheets>

    <Text fx:id="name" GridPane.columnIndex="0" GridPane.rowIndex="0"/>

    <Label text="Effective Happiness:" GridPane.columnIndex="0" GridPane.rowIndex="2"/><Text fx:id="effectivehappiness" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
    <Label text="Education:" GridPane.columnIndex="0" GridPane.rowIndex="3"/><Text fx:id="edu" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
    <Label text="Deposit:" GridPane.columnIndex="0" GridPane.rowIndex="4"/><Text fx:id="deposit" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
    <Label text="Gross Income:" GridPane.columnIndex="0" GridPane.rowIndex="5"/><Text fx:id="grossincome" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
    <Label text="Works at:" GridPane.columnIndex="0" GridPane.rowIndex="6"/><Text fx:id="worksat" GridPane.columnIndex="1" GridPane.rowIndex="6"/>

    <Button fx:id="back" text="back" GridPane.columnIndex="1" GridPane.rowIndex="7"/>

</GridPane>


